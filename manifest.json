{
  "name": "Simple Link Clipper",
  "version": "0.1.0",
  "manifest_version": 2,
  "description": "Clip url in the selected format.",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "background": {
    "scripts": ["lib/background.js"],
    "persistent": false
  },
  "commands": {
    "EventName": {
      "suggested_key": {
        "default": "Alt+C"
      },
      "description": "Copy to clipboard trigger."
    }
  },
  "page_action": {
    "default_icon": "assets/img/icon/19.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "assets/img/icon/128.png"
  }
}
